{
    "python.defaultInterpreterPath": "./tire-rag-env/bin/python",
    "python.terminal.activateEnvironment": true,
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.banditEnabled": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": ["--line-length", "88"],
    "python.sortImports.args": ["--profile", "black"],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/node_modules": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true,
        "**/tire-rag-env": false
    },
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "testing"
    ],
    "python.testing.unittestEnabled": false,
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.runStartupCommands": [
        "%load_ext autoreload",
        "%autoreload 2"
    ],
    "yaml.schemas": {
        "./config/agent_config.yaml": [
            "config/agent_config.yaml"
        ],
        "./config/rag_config.yaml": [
            "config/rag_config.yaml"
        ],
        "./config/security_config.yaml": [
            "config/security_config.yaml"
        ]
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/data/raw": true,
        "**/data/processed": true,
        "**/models": true,
        "**/.git": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/tire-rag-env": true
    },
    "files.associations": {
        "*.yaml": "yaml",
        "*.yml": "yaml",
        "requirements*.txt": "pip-requirements",
        ".env*": "dotenv",
        "Dockerfile*": "dockerfile"
    },
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "terminal.integrated.env.osx": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "terminal.integrated.env.windows": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "python.envFile": "${workspaceFolder}/.env",
    "rest-client.environmentVariables": {
        "local": {
            "baseUrl": "http://localhost:8000",
            "token": "your_token_here"
        },
        "dev": {
            "baseUrl": "https://dev-tire-rag.example.com",
            "token": "your_dev_token_here"
        }
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#1e3a8a",
        "activityBar.foreground": "#ffffff",
        "statusBar.background": "#1e40af",
        "statusBar.foreground": "#ffffff",
        "titleBar.activeBackground": "#1e40af",
        "titleBar.activeForeground": "#ffffff"
    },
    "workbench.colorTheme": "Default Dark+",
    "editor.minimap.enabled": true,
    "editor.rulers": [88, 120],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 88,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "extensions.ignoreRecommendations": false,
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.autoSearchPaths": true,
    "python.analysis.diagnosticMode": "workspace",
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "PRODUCTION",
        "SECURITY",
        "PERFORMANCE"
    ],
    "todo-tree.highlights.customHighlight": {
        "PRODUCTION": {
            "icon": "alert",
            "type": "line",
            "foreground": "#ffffff",
            "background": "#dc2626",
            "iconColour": "#dc2626"
        },
        "SECURITY": {
            "icon": "shield",
            "type": "line", 
            "foreground": "#ffffff",
            "background": "#ea580c",
            "iconColour": "#ea580c"
        },
        "PERFORMANCE": {
            "icon": "zap",
            "type": "line",
            "foreground": "#ffffff", 
            "background": "#7c3aed",
            "iconColour": "#7c3aed"
        }
    },
    "thunder-client.workspaceRelativePath": "api-tests",
    "thunder-client.saveRequestOnSend": true,
    "cSpell.words": [
        "agentic",
        "graphrag",
        "apollo",
        "tyres",
        "ultralytics",
        "chromadb",
        "crewai",
        "langchain",
        "ollama",
        "streamlit",
        "plotly",
        "pydantic",
        "fastapi",
        "uvicorn",
        "pytest",
        "bandit",
        "mypy",
        "flake8",
        "isort",
        "owasp",
        "ssrf",
        "csrf",
        "xss",
        "curing",
        "vulcanization",
        "sidewall",
        "tread"
    ],
    "markdown.preview.fontSize": 14,
    "markdown.preview.lineHeight": 1.6,
    "redhat.telemetry.enabled": false,
    "telemetry.telemetryLevel": "off"
}